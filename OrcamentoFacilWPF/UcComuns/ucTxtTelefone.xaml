<UserControl x:Class="OrcamentoFacilWPF.UcTxtTelefone"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:OrcamentoFacilWPF" xmlns:wf="clr-namespace:System.Windows.Forms;assembly=System.Windows.Forms"
             mc:Ignorable="d" Height="21" Width="108">
    <Grid>
        <WindowsFormsHost FontFamily="Tahoma" FontSize="12">
            <wf:MaskedTextBox x:Name="txtTelefone" Mask="(99)99999-9999" BackColor="#d6d6d6" ForeColor="White" Font="Tahoma, 12" BorderStyle="FixedSingle"/>
        </WindowsFormsHost>            
    </Grid>
</UserControl>
