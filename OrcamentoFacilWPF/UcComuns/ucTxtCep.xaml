<UserControl x:Class="OrcamentoFacilWPF.ucTxtCep"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:OrcamentoFacilWPF" xmlns:wf="clr-namespace:System.Windows.Forms;assembly=System.Windows.Forms"
             mc:Ignorable="d" Height="21" Width="108">
    <Grid>
        <WindowsFormsHost>
            <wf:MaskedTextBox x:Name="txtCep" Mask="99999-999" BackColor="#d6d6d6" ForeColor="White" Font="Tahoma, 12" BorderStyle="FixedSingle" />
        </WindowsFormsHost>
    </Grid>
</UserControl>
